<h1><%= @house.name %></h1>

<ul>
  <%= @house.author %>
</ul>

<ul>
  <%= @house.source %>
</ul>

<ul>
  <%= @house.motto %>
</ul>

<h4>House Members</h4>

<ul>
  <% @members.each do |member| %>
    <li>
      <%= member.first_name %>
    </li>
    <li>
      <%= member.last_name %>
    </li>
    <li>
      <%= member.created_at.strftime("%B %d %Y") %>
    </li>
  <% end %>
</ul>

  <%= link_to 'Add a House Member', new_house_member_path(@house) %>
